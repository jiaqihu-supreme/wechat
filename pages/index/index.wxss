.index_swiper swiper {
  width: 750rpx;
  height: 340rpx;
}
.index_swiper swiper image{
  width: 100%;
}
.index_cate {
  display: flex;
  margin-top: 10rpx;
}
.index_cate navigator {
  flex: 1;
  text-align: center;
}
.index_cate navigator image {
  width: 80%;
}
.index_floor{
  margin-top: 20rpx;
}
.floor_group .floor_title image {
  width: 100%;
}
.floor_content {
  overflow: hidden;
}
.floor_content navigator {
  float: left;
  width: 33.33%;
}
.floor_content navigator image {
  width: 100%;
  height: 100%;
}
.floor_content navigator image:nth-last-child(-n+4){
  height: calc(33.33vw * 360 / 232 / 2);
  border-left: 10rpx solid #fff;
}
.floor_content navigator:nth-child(2),
.floor_content navigator:nth-child(3){
  border-bottom: 10rpx solid #fff;
}
.floor_content navigator:nth-child(3),
.floor_content navigator:nth-child(5){
  border-right: 10rpx solid #fff;
}

